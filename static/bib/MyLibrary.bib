  url = {https://jamanetwork.com/journals/jamapsychiatry/fullarticle/207731},
  doi = {10.1001/archpsyc.60.8.828},
  shorttitle = {Gambling {{Urges}} in {{Pathological Gambling}}},
  abstract = {{$<$}h3{$>$}Background{$<$}/h3{$><$}p{$>$}Gambling urges in pathological gambling (PG) often immediately precede engagement in self-destructive gambling behavior. An improved understanding of the neural correlates of gambling urges in PG would advance our understanding of the brain mechanisms underlying PG and would help direct research into effective treatments.{$<$}/p{$><$}h3{$>$}Methods{$<$}/h3{$><$}p{$>$}Echoplanar functional magnetic resonance imaging was used to assess brain function during viewing of videotaped scenarios with gambling, happy, or sad content. Participants rated the quality and magnitude of their emotional and motivational responses.{$<$}/p{$><$}h3{$>$}Results{$<$}/h3{$><$}p{$>$}Men with PG (n = 10) reported mean ± SD greater gambling urges after viewing gambling scenarios vs control subjects (n = 11) (5.20 ± 3.43 vs 0.32 ± 0.60; χ\textsuperscript{2}\textsubscript{1,19}= 21.71;\emph{P}\&lt;.001). The groups did not differ significantly in their subjective responses to the happy (\emph{P}= .56) or sad (\emph{P}= .81) videotapes. The most pronounced between-group differences in neural activities were observed during the initial period of viewing of the gambling scenarios: PG subjects displayed relatively decreased activity in frontal and orbitofrontal cortex, caudate/basal ganglia, and thalamus compared with controls. Distinct patterns of regional brain activity were observed in specific temporal epochs of videotape viewing. For example, differences localized to the ventral anterior cingulate during the final period of gambling videotape viewing, corresponding to the presentation of the most provocative gambling stimuli. Although group differences in brain activity were observed during viewing of the sad and happy scenarios, they were distinct from those corresponding to the gambling scenarios.{$<$}/p{$><$}h3{$>$}Conclusions{$<$}/h3{$><$}p{$>$}In men with PG, gambling cue presentation elicits gambling urges and leads to a temporally dynamic pattern of brain activity changes in frontal, paralimbic, and limbic brain structures. When viewing gambling cues, PG subjects demonstrate relatively decreased activity in brain regions implicated in impulse regulation compared with controls.{$<$}/p{$>$}},
  number = {8},
  journaltitle = {Archives of General Psychiatry},
  shortjournal = {Arch Gen Psychiatry},
  urldate = {2018-11-21},
  date = {2003-08-01},
  pages = {828-836},
  author = {Potenza, Marc N. and Steinberg, Marvin A. and Skudlarski, Pawel and Fulbright, Robert K. and Lacadie, Cheryl M. and Wilber, Mary K. and Rounsaville, Bruce J. and Gore, John C. and Wexler, Bruce E.},
  file = {/home/cillian/Google/Zotero/storage/64PYBA8K/Potenza et al_2003_Gambling Urges in Pathological Gambling.pdf;/home/cillian/Google/Zotero/storage/ME9RAV9M/207731.html}
}

@article{stanton_testosterone_2011,
