  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/B87V867H/Schnell_2011_Individual differences in meaning-making.pdf;/media/cillian/intel250G/Home/Google/Zotero/storage/SSVPNARE/S0191886911002820.html}
}

