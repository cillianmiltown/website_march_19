}

@article{mercier_argumentative_2016,
  title = {The {{Argumentative Theory}}: {{Predictions}} and {{Empirical Evidence}}},
  shorttitle = {The {{Argumentative Theory}}},
  author = {Mercier, Hugo},
  year = {2016},
  month = sep,
  journal = {Trends in Cognitive Sciences},
  volume = {20},
