  keywords = {*Disgust,*Facial Expressions,*Language,*Morality,Childhood Development,Unread},
  annotation = {00044},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/WAT7A4PF/Danovitch_Bloom_2009_Children's extension of disgust to physical and moral events.pdf}
}

