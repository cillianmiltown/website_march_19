  pages = {5400-5408},
  keywords = {Humans,Rats,Adenosine Triphosphate,Hydrogen-Ion Concentration,Animals,Calcium,Cell Membrane,Citrates,Erythrocyte Aging,Erythrocytes,Glucose,Guinea Pigs,Haplorhini,Hemolysis,Magnesium,Phosphatidylcholines,Phospholipases,Phospholipids,Potassium,Snake Venoms,Swine,Time Factors},
  author = {Martin, J. K. and Luthra, M. G. and Wells, M. A. and Watts, R. P. and Hanahan, D. J.},
  eprinttype = {pmid},
  eprint = {54}
}

