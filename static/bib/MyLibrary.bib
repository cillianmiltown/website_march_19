@article{inzlicht_reflecting_2010,
  title = {Reflecting on {{God Religious Primes Can Reduce Neurophysiological Response}} to {{Errors}}},
  author = {Inzlicht, Michael and Tullett, Alexa M.},
  year = {2010},
  month = aug,
  journal = {Psychological Science},
  volume = {21},
  number = {8},
  pages = {1184--1190},
  issn = {0956-7976, 1467-9280},
  doi = {10.1177/0956797610375451},
  abstract = {The world is a vast and complex place that can sometimes generate feelings of uncertainty and distress for its inhabitants. Although religion is associated with a sense of meaning and order, it remains unclear whether religious belief can actually cause people to feel less anxiety and distress. To test the anxiolytic power of religion, we conducted two experiments focusing on the error-related negativity (ERN)\textemdash a neural signal that arises from the anterior cingulate cortex and is associated with defensive responses to errors. The results indicate that for believers, conscious and nonconscious religious primes cause a decrease in ERN amplitude. In contrast, priming nonbelievers with religious concepts causes an increase in ERN amplitude. Overall, examining basic neurophysiological processes reveals the power of religion to act as a buffer against anxious reactions to self-generated, generic errors\textemdash but only for individuals who believe.},
  language = {en},
  pmid = {20558751},
  keywords = {1 line summary,error-related negativity,Meaning,neuroscience,religion},
  annotation = {00091},
