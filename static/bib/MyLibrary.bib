  pages = {757-773},
  keywords = {Moral judgment,Morality,Ethics,Unread,judgment,unethical behavior,Cognitive Dissonance,ethical dissonance,impression management},
  author = {Barkan, Rachel and Ayal, Shahar and Gino, Francesca and Ariely, Dan},
