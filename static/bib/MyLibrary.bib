@article{hauser_dissociation_2007,
  title = {A {{Dissociation Between Moral Judgments}} and {{Justifications}}},
  author = {Hauser, Marc D. and Cushman, Fiery A. and Young, Liane and {Kang-Xing Jin}, R. and Mikhail, John},
  year = {2007},
