}

@article{weiser_effect_2010,
  title = {Effect of {{A}} 19-{{Item Surgical Safety Checklist During Urgent Operations}} in {{A Global Patient Population}}},
  author = {Weiser, Thomas G. and Haynes, Alex B. and Dziekan, Gerald and Berry, William R. and Lipsitz, Stuart R. and Gawande, Atul A. and Group, for the Safe Surgery Saves Lives Investigators {and} Study},
  year = {2010},
  month = may,
