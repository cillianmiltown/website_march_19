@article{lane_bleedingheart_2017,
  title = {Bleeding-Heart Conservatives and Hard-Headed Liberals: {{The}} Dual Processes of Moral Judgements},
  shorttitle = {Bleeding-Heart Conservatives and Hard-Headed Liberals},
  author = {Lane, Dylan and Sulikowski, Danielle},
  year = {2017},
