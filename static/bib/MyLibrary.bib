@techreport{cowley_when_2005,
  type = {{{SSRN Scholarly Paper}}},
  title = {When {{Falsification}} Is the {{Only Path}} to {{Truth}}},
  author = {Cowley, Michelle B. and Byrne, Ruth M. J.},
  year = {2005},
  month = feb,
  number = {ID 2339585},
  address = {{Rochester, NY}},
  institution = {{Social Science Research Network}},
  abstract = {Can people consistently attempt to falsify, that is, search for refuting evidence, when testing the truth of hypotheses? Experimental evidence indicates that people tend to search for confirming evidence. We report two novel experiments that show that people can consistently falsify when it is the only helpful strategy. Experiment 1 showed that participants readily falsified somebody else's hypothesis.  Their task was to test a hypothesis belonging to an `imaginary participant' and they knew it was a low quality hypothesis. Experiment 2 showed that participants were able to falsify a low quality hypothesis belonging to an imaginary participant more readily than their own low quality hypothesis. The results have important implications for theories of hypothesis testing and human rationality.},
  keywords = {Michelle B. Cowley PGDipStat BA DPhil,Ruth M.J. Byrne,SSRN,When Falsification is the Only Path to Truth},
  annotation = {00000},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/BHTHTNSS/papers.html}
}

