  keywords = {SYSNEURO},
  author = {Clark, Luke and Lawrence, Andrew J. and Astley-Jones, Frances and Gray, Nicola},
  file = {/home/cillian/Google/Zotero/storage/FCZJQSPT/Clark et al_2009_Gambling Near-Misses Enhance Motivation to Gamble and Recruit Win-Related Brain.pdf;/home/cillian/Google/Zotero/storage/JHHLNV6V/S0896627309000373.html}
