  keywords = {1 line summary,Emotion,Empathy,faces,human,Simulation,social,somatic,somatosensory},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/2GDACX8X/Adolphs et al. - 2000 - A Role for Somatosensory Cortices in the Visual Re.pdf;/media/cillian/intel250G/Home/Google/Zotero/storage/F7H56SBG/2683.html}
}

