@article{sturman_dehumanizing_2012,
  title = {Dehumanizing Just Makes You Feel Better: {{The}} Role of Cognitive Dissonance in Dehumanization},
  shorttitle = {Dehumanizing Just Makes You Feel Better},
  author = {Sturman, Edward D.},
  year = {2012},
  month = dec,
  journal = {Journal of Social, Evolutionary, and Cultural Psychology},
  volume = {6},
  number = {4},
  pages = {527--531},
  issn = {1933-5377},
  doi = {10.1037/h0099231},
  abstract = {Reviews the book, "Less Than Human: Why We Demean, Enslave, and Exterminate Others" by David Livingstone Smith (2011). In "Less than Human", David Livingstone Smith conducts a much-needed investigation of dehumanization, which seems to be the mechanism by which humans are able to drop their inhibitions against doing harm to others and commit unspeakable acts of cruelty and horror. The book is rife with vivid examples of atrocities, which appear to be facilitated by the perpetrators' ability to think of the victims as subhuman. Smith is not content merely to describe the phenomena; he traces the history of the concept and puts forward a well-conceived theory behind dehumanization based on scientific evidence. Although the book is decidedly philosophical, it should have strong appeal to psychologists with a background in social or evolutionary psychology, as much of the scientific evidence is rooted in these fields. Before covering the content, it should be noted that the quality of the writing and the style of the book make it eminently readable. Smith incorporates many vivid examples and stories that capture the reader and are sure to evoke a visceral response; and yet there is a strong commitment to explaining underlying processes and theories. Readers from a variety of backgrounds will find the book both informative and engaging. The book opens with a series of examples of dehumanization from the Second World War. From Nazi doctors performing lethal and torturous medical experiments on prisoners to the atrocities committed by all sides in the war, we see how soldiers and even doctors were persuaded to think of different ethnicities as subhuman. Smith recounts horrific stories of abuses by all sides, aided by official portrayals of the enemy as animals. An underlying theme here is that the atrocities in the war were not committed by monsters or psychopaths who bear little resemblance to us; they were committed by regular people who would otherwise not be able to bring themselves to perform such acts in civilized society. What is it that enabled them to do so in the context of war? Smith argues that it was their ability to think of their victims as subhuman and therefore not deserving the basic consideration that would ordinarily be afforded a fellow human being. In conclusion, the reviewer states that Smith has written a compelling and thoughtful account of dehumanization that is highly recommended for lay people and professionals alike. We can hope that by having a better handle on some of the factors involved in dehumanization, we may one day be able to diminish its destructive effects. In the final chapter, Smith calls for a scientific approach to dehumanization and empirical study of the subject. The reviewer suggests that two areas of study that might bear fruit are the underlying motivations behind dehumanization and the role of cognitive dissonance, which may be a key mechanism in the dehumanization process. (PsycINFO Database Record (c) 2014 APA, all rights reserved)},
  keywords = {Behavioral Disinhibition,Cognitive Dissonance,dehumanization,disinhibition,evolutionary psychology,Morality,Philosophies,Philosophy,Unread},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/ZPGMN8FR/Sturman_2012_Dehumanizing just makes you feel better.pdf}
}

