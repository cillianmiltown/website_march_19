@article{jenkins_learning_2021,
  title = {Learning Words in Space and Time: {{Contrasting}} Models of the Suspicious Coincidence Effect},
  shorttitle = {Learning Words in Space and Time},
  author = {Jenkins, Gavin W. and Samuelson, Larissa K. and Penny, Will and Spencer, John P.},
  year = {2021},
