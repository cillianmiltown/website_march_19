  date = {2018-12-06},
  author = {Ghelfi, Eric and Christopherson, Cody D and Urry, Heather L. and Lenne, Richie L and Legate, Nicole and Fischer, Mary Ann and Wagemans, Fieke Maria Antoinet and Wiggins, Bradford Jay and Barrett, Tamara and Bornstein, Michelle and de Haan, Bianca and Guberman, Joshua B. and Issa, Nada and Kim, Joan and Na, Elim and O'Brien, Justin and Paulk, Aidan and Peck, Tayler and Sashihara, Marissa and Sheelar, Karen and Song, Justin and Steinberg, Hannah and Sullivan, Dasan},
  options = {useprefix=true},
  file = {/home/cillian/Google/Zotero/storage/GEYDWAAY/Ghelfi et al_2018_Reexamining the effect of gustatory disgust on moral judgment.pdf}
