  keywords = {culture,dilemma,morality},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/8NSCC3G7/Awad et al_2020_Universals and variations in moral decisions made in 42 countries by 70,000.pdf;/media/cillian/intel250G/Home/Google/Zotero/storage/K7TXJ57F/2332.html}
