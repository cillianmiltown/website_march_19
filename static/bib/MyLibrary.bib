  urldate = {2019-04-01},
  date = {2017},
  pages = {971-985},
  keywords = {Adult,Female,Humans,Male,Young Adult,Adolescent,Time Perception,Psychomotor Performance},
  author = {Fereday, Richard and Buehner, Marc J.},
