@inproceedings{mchugh_nature_,
  title = {The Nature of Category Formation},
  abstract = {This paper will present a case for category formation as a consequence of goal directed behaviour, where stability in category formation depends on (i) stable/frequent behaviours; (ii) encountering natural kinds; and (iii) social norms. This account of category formation will build on Barsalou’s situated simulation theory (2003). Situated simulation theory is presented across the five dimensions of (i) Architecture (non-modular as opposed to modular); (ii) Representation (modal as opposed to amodal); (iii) Abstraction (situated as opposed to decontextualised); (iv) Stability (dynamical as opposed to stable); and (v) Organisation (action-environment interface as opposed to taxonomical). Using these same dimensions, this paper will present a non-modular conceptual system with modal, situated, dynamical representations. Dynamical representations assume that representations are not stable across people or within a person; situated representations mean that contextual information is included in the representation. It follows from these assumptions that categories cannot be taxonomically organised, as taxonomical rules do not account for contextual information and should produce stability. This means the traditional dimension of organisation assumptions are in need of review. Barsalou (2003) addresses this with his action-environment interface model. In this model, Barsalou attempts to provide an account for the “mapping” between knowledge about the environment and knowledge about action (Barsalou, 2003). This paper will suggest that the action-environment interface is an unnecessarily complicated way of accounting for category formation as a result of goal directed behaviour. Instead, drawing on a range of works (Barsalou, 1999, 2003; Vallee-Tourangeau, Anthony, \& Austin, 1998; Walker \& Kintsch, 1985), it is argued that ad-hoc goal-derived categories are formed when engaging in goal driven behaviour. Stability in category formation emerges as a consequence of stability in goal driven behaviours. Stable categories also emerge as a result of encountering natural kinds and social norms.},
  author = {McHugh, Cillian},
  file = {/home/cillian/Dropbox/College/research/notes/draft 2/the nature of category formation.doc;/home/cillian/Google/Zotero/storage/J6RA5WGD/Situated simulation theory a critical review.docx;/home/cillian/Google/Zotero/storage/UTD6NPD8/Situated simulation theory a critical review (first draft) - with comments.doc;C\:\\Users\\cillian.mchugh\\Dropbox\\College\\research\\notes\\draft 2\\the nature of category formation.doc}
}

