@article{rachman_experimental_1988,
  title = {Experimental Analyses of Panic\textemdash{{II}}. {{Panic}} Patients},
  author = {Rachman, S. and Lopatka, C. and Levitt, K.},
  year = {1988},
  month = jan,
  journal = {Behaviour Research and Therapy},
  volume = {26},
  number = {1},
  pages = {33--40},
  issn = {0005-7967},
  doi = {10.1016/0005-7967(88)90031-9},
  abstract = {An attempt was made to replicate on 20 panic patients the findings obtained in a study of panic-induction among claustrophobic Ss. Many of the findings were similar. Both samples of Ss tended to overpredict the probability of experiencing a panic, and their panic trials were accompanied by many more bodily symptoms and many more fearful cognitions than were the no-panic trials. In both samples. understandable links between cognitions, symptoms and panic episodes emerged. However, the content of the fearful cognitions differed between the samples. In both samples, expected panics and disconfirmed panics were followed by declines in predicted and reported panic or fear. Panics can be induced by psychological methods, in or out of a laboratory, with fears of differing content, and regardless of the psychiatric status of the Ss.},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/TCRT3EGW/0005796788900319.html}
}

@article{rachman_match_1986,
  title = {Match and Mismatch in the Prediction of Fear\textemdash{{I}}},
  author = {Rachman, S. and Lopatka, C.},
  year = {1986},
  month = jan,
  journal = {Behaviour Research and Therapy},
  volume = {24},
  number = {4},
  pages = {387--393},
  issn = {0005-7967},
  doi = {10.1016/0005-7967(86)90003-3},
  abstract = {An experiment was carried out on 44 fearful Ss in order to find out how accurately people can predict their fear responses, and whether they become more accurate with practice. We also examined the effects on predicted and reported fear of correct matches, over-predicted mismatches and underpredicted mismatches. The Ss tended to over-predict their fear responses, and did learn to predict more accurately with practice. Predicted fear tended to increase after under-predictions, and tended to decrease after over-predictions. After a correct match, predictions of fear remained unchanged. Reports of fear declined steadily, and showed little relation to the occurrence of correct or incorrect matches.},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/6XC7SGRI/0005796786900033.html}
}

@article{rachman_modification_1983,
  title = {The Modification of Agoraphobic Avoidance Behaviour: {{Some}} Fresh Possibilities},
  shorttitle = {The Modification of Agoraphobic Avoidance Behaviour},
  author = {Rachman, S.},
  year = {1983},
  month = jan,
  journal = {Behaviour Research and Therapy},
  series = {Special {{Announcement}}},
  volume = {21},
  number = {5},
  pages = {567--574},
  issn = {0005-7967},
  doi = {10.1016/0005-7967(83)90048-7},
  abstract = {A reconsideration of the theoretical basis of the therapy provided for agoraphobic people is warranted on three grounds. Although current methods of behavioural treatment are moderately effective, many patients are left with significant residual problems. Secondly, consideration of the theoretical basis of current behavioural treatments has been neglected and thirdly, there has been insufficient curiosity about the nature of agoraphobia. Three approaches to the problem of persistent avoidance behaviour and/or fear were selected as being of particular promise: Seligman and Johnston's cognitive theory of avoidance behaviour. Gray's elaboration of the safety signal hypothesis and Bandura's self-efficacy theory. Proposals for two new forms of therapy were formulated from an examination of these theories. The most promising possibilities are `therapy by repeated disconfirmation' and `safety-signal therapy'. The general lines of these new forms of therapy are set out, examples provided and suggestions put forward for assessment procedures. It is suggested that in many agoraphobic cases, the control of panic attacks is a target of central importance and renewed attempts to find dependable ways of controlling panic attacks by psychological methods should be made. It is also recommended that self-efficacy estimates should be included in experimental investigations of the therapy provided for people with agoraphobic problems.},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/D7ENY6K3/0005796783900487.html}
}

@article{rachman_overprediction_1988,
  title = {The Overprediction of Fear},
  author = {Rachman, S. and Bichard, S.},
  year = {1988},
  month = jan,
  journal = {Clinical Psychology Review},
  volume = {8},
  number = {3},
  pages = {303--312},
  issn = {0272-7358},
  doi = {10.1016/0272-7358(88)90093-1},
  abstract = {The overprediction of fear is common, and fearful people are particularly prone to overestimate how much fear they will experience. Predictions of fear tend to increase after underpredictions, to decrease after over predictions, and to remain constant after a correct match. Predictions of fear tend to become more accurate with practice. These features of predictions and their consequences have also been reported in studies of panic. Some features of the overprediction of pain resemble the findings on fear, and it is possible that the main properties of overprediction patterns are common to all aversive events/experiences. The functional value of the overprediction of fear is discussed, and clinical implications are considered.},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/NCRF4PII/0272735888900931.html}
}

@article{rachman_predicting_1989,
  title = {Predicting and Remembering Recurrent Pain},
  author = {Rachman, S. and Eyrl, K.},
  year = {1989},
  month = jan,
  journal = {Behaviour Research and Therapy},
  volume = {27},
  number = {6},
  pages = {621--635},
  issn = {0005-7967},
  doi = {10.1016/0005-7967(89)90146-0},
  abstract = {Two groups of Ss suffering from recurrent episodes of pain (headache or menstrual) were studied over a period of weeks in order to determine if they tend to over-predict aversive events (pain in this instance). The results are consistent with earlier findings on fear pain. Contrary to earlier findings, however, after an under-predicted pain the menstrual Ss decreased their following prediction\textemdash suggesting the operation of a superordinate predictive pattern in this group. The Ss in both groups successfully predicted the qualities of their pain episodes, but tended to recall the episodes as being more painful than they had reported at the time of the pain episode. The explanation for the erroneous magnifications of predictions and reports of pain is not obvious. Over-predicting a potential pain, and remembering an episode as having been more painful than it was, may serve a protective function, but if so, these are not the most economical ways of achieving protection. The Ss who used medication expected to obtain moderate relief and they were not disappointed.},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/WAKTRGHB/0005796789901460.html}
}

