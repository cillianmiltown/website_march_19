  file = {/home/cillian/Google/Zotero/storage/3J6NHFR9/Gray et al_2017_How to think about emotion and morality.pdf;/home/cillian/Google/Zotero/storage/IGTJ3GE8/S2352250X16302305.html}
}

@article{gray_two_2012,
  langid = {english},
  title = {Two {{Minds Vs}}. {{Two Philosophies}}: {{Mind Perception Defines Morality}} and {{Dissolves}} the {{Debate Between Deontology}} and {{Utilitarianism}}},
  volume = {3},
  issn = {1878-5158, 1878-5166},
  url = {http://link.springer.com/article/10.1007/s13164-012-0112-5},
  doi = {10.1007/s13164-012-0112-5},
  shorttitle = {Two {{Minds Vs}}. {{Two Philosophies}}},
  abstract = {Mind perception is the essence of moral judgment. Broadly, moral standing is linked to perceptions of mind, with moral responsibility tied to perceived agency, and moral rights tied to perceived experience. More specifically, moral judgments are based on a fundamental template of two perceived minds—an intentional agent and a suffering patient. This dyadic template grows out of the universal power of harm, and serves as a cognitive working model through which even atypical moral events are understood. Thus, all instances of immorality are perceived to involve both blameworthy agents (i.e., acts) and suffering victims (i.e., consequences). Because moral cognition simultaneously concerns acts and consequences, theories which focus primarily on acts (i.e., deontology) or consequences (i.e., utilitarianism) do not accurately describe moral cognition. Indeed, the phenomenon of dyadic completion suggests that deontological and utilitarian concerns are not only simultaneously active, but also typically compatible and reinforcing: wrong acts have harmful consequences, and harmful consequences stem from wrong acts. The cognitive fusion of acts with consequences suggests that normative conflicts between deontology and utilitarianism are not reflected in everyday moral judgment. This in turn suggests that empirical conclusions drawn from moral dilemmas that pit utilitarianism against deontology—i.e., trolley problems—give an skewed account of moral cognition.},
  number = {3},
  journaltitle = {Review of Philosophy and Psychology},
  shortjournal = {Rev.Phil.Psych.},
  urldate = {2015-03-05},
  date = {2012-09-18},
  pages = {405-423},
  keywords = {Epistemology,Philosophy of Mind,Cognitive Psychology,Neurosciences,1 line summary,Philosophy of Science,Developmental Psychology},
  author = {Gray, Kurt James and Schein, Chelsea},
  file = {/home/cillian/Google/Zotero/storage/4I2MCAX9/Gray_Schein_2012_Two Minds Vs.pdf;/home/cillian/Google/Zotero/storage/CU9DU6XI/10.html},
  note = {00013}
}

