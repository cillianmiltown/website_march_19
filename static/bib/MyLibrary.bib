@article{murphy_categorizing_1989,
  title = {Categorizing Objects in Isolation and in Scenes: {{What}} a Superordinate Is Good For},
  shorttitle = {Categorizing Objects in Isolation and in Scenes},
  author = {Murphy, Gregory L. and Wisniewski, Edward J.},
  year = {1989},
  journal = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
  volume = {15},
  number = {4},
  pages = {572--586},
  issn = {1939-1285(Electronic);0278-7393(Print)},
  doi = {10.1037/0278-7393.15.4.572},
  abstract = {Many studies have shown that subjects are faster at categorizing objects into "basic" concepts than into more general superordinate concepts. However, all of these studies have used a categorization task in which single, isolated objects are identified. There is good reason to believe that superordinate concepts are typically used to refer to collections of objects rather than to individual objects. For example, people more often use the term furniture to refer to a number of pieces of furniture rather than to name a single piece. This suggests that superordinate concepts include information about multiple objects and their common relations, particularly the typical scenes in which such objects appear. Four experiments examined this possibility by investigating whether the basic concept advantage will decrease or reverse itself when subjects are asked to categorize an object as part of a scene. The results showed that the basic-superordinate difference did decrease when subjects categorized objects in scenes. Furthermore, when an object was placed in an inappropriate scene, there was more interference for superordinate identifications. The results suggest qualitative differences in the representations of superordinate and basic concepts.},
  copyright = {(c) 2012 APA, all rights reserved},
  keywords = {*Classification (Cognitive Process),*Contextual Associations,Concepts}
}

@incollection{murphy_explanatory_2000,
  title = {Explanatory Concepts},
  booktitle = {Explanation and Cognition},
  author = {Murphy, Gregory L.},
  editor = {Keil, Frank C. and Wilson, R. A.},
  year = {2000},
  pages = {361--392},
  publisher = {{The MIT Press}},
  address = {{Cambridge,  MA,  US}},
  abstract = {The author examines how explanatory knowledge, in contrast to knowledge of simple facts or other shallower aspects of understanding, influences a variety of aspects of everyday cognition, most notably the ability to learn new categories. An explanatory schema that helps explain some features in a new category has a kind of penumbra that aids acquisition of other features not causally related to those for which there are explanations. Somehow, explanatory structure confers cognitive benefits in ways that extend beyond features immediately relevant to that structure. The author argues that this makes sense, given how often, at least for natural categories, many features are learned that have no immediately apparent causal role. Features that fit into explanatory relations are seen as more typical to a category even when they occur much less often than other explanatorily irrelevant features. Such results strongly indicate that explanation does not just come in at the tail end of concept learning. In many cases, it guides concept learning from the start and in ways that can be quite different from accounts that try to build knowledge out of simple feature frequencies and correlations.},
  copyright = {(c) 2012 APA, all rights reserved},
  isbn = {0-262-11249-3 (Hardcover)},
  keywords = {*Cognition,*Concepts,*Interpersonal Communication,*Knowledge Level,*Learning,Reasoning},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/P82X2353/2000-05066-014.html}
}

@article{murphy_role_1985,
