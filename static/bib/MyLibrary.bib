  keywords = {*Cross Cultural Differences,*Emotion Recognition,*Face Perception,*Facial Expressions,1 line summary,Emotions},
  annotation = {00012},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/B2WWIUUM/Gendron et al_2014_Perceptions of emotion from facial expressions are not culturally universal.pdf}
}

