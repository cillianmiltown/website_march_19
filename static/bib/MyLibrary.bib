  author = {McGuire, Jonathan and Langdon, Robyn and Coltheart, Max and Mackenzie, Catriona},
  year = {2009},
  month = may,
  journal = {Journal of Experimental Social Psychology},
  volume = {45},
  number = {3},
  pages = {577--580},
  issn = {0022-1031},
