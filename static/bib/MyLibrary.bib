@article{kruglanski_persuasion_1999,
  ids = {kruglanski_persuasion_1999a},
  title = {Persuasion by a {{Single Route}}: {{A View From}} the {{Unimodel}}},
  shorttitle = {Persuasion by a {{Single Route}}},
