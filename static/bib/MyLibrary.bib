  issn = {0022-1031},
  doi = {10.1016/j.jesp.2009.01.003},
  abstract = {A substantial body of research supports a dual-process theory of moral judgment, according to which characteristically deontological judgments are driven by automatic emotional responses, while characteristically utilitarian judgments are driven by controlled cognitive processes. This theory was initially supported by neuroimaging and reaction time (RT) data. McGuire et al. have reanalyzed these initial RT data and claim that, in light of their findings, the dual-process theory of moral judgment and the personal/impersonal distinction now lack support. While McGuire and colleagues have convincingly overturned Greene et al.'s interpretation of their original RT data, their claim that the dual-process theory now lacks support overstates the implications of their findings. McGuire and colleagues ignore the results of several more recent behavioral studies, including the study that bears most directly on their critique. They dismiss without adequate justification the results of a more recent neuroimaging study, three more recent patient studies, and an emotion\textendash induction study. Their broader critique is based largely on their conflation of the dual-process theory with the personal/impersonal distinction, which are independent.},
  keywords = {Dual-process,Moral judgment,Moral psychology},
  annotation = {00135},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/AXS59PJU/Greene_2009_Dual-process morality and the personal-impersonal distinction.pdf;/media/cillian/intel250G/Home/Google/Zotero/storage/SGSTTBEH/Greene_2009_Dual-process morality and the personal-impersonal distinction.pdf;/media/cillian/intel250G/Home/Google/Zotero/storage/4EDD5NAU/S0022103109000262.html;/media/cillian/intel250G/Home/Google/Zotero/storage/XDQU88DK/S0022103109000262.html}
