  shorttitle = {The Trauma Model of Dissociation},
  author = {Lynn, Steven Jay and Lilienfeld, Scott O. and Merckelbach, Harald and Giesbrecht, Timo and McNally, Richard J. and Loftus, Elizabeth F. and Bruck, Maggie and Garry, Maryanne and Malaktaris, Anne},
  year = {2014},
