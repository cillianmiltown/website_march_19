@article{bostyn_bright_2020,
  title = {Bright Mind, Moral Mind? {{Intelligence}} Is Unrelated to Consequentialist Moral Judgment in Sacrificial Moral Dilemmas},
  shorttitle = {Bright Mind, Moral Mind?},
  author = {Bostyn, Dries H. and De Keersmaecker, J. and Van Assche, J. and Roets, Arne},
  year = {2020},
  month = apr,
  journal = {Psychonomic Bulletin \& Review},
