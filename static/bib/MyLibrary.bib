  keywords = {phenomenologic inquiry,quarantine,Severe acute respiratory syndrome (SARS)},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/G7Y4E7EZ/Lin et al_2010_Lessons learned from the anti-SARS quarantine experience in a hospital-based.pdf;/media/cillian/intel250G/Home/Google/Zotero/storage/ZBCK2XRA/S0196655309009390.html}
