@article{lieberman_does_2003,
  title = {Does Morality Have a Biological Basis? {{An}} Empirical Test of the Factors Governing Moral Sentiments Relating to Incest},
  shorttitle = {Does Morality Have a Biological Basis?},
  author = {Lieberman, Debra and Tooby, John and Cosmides, Leda},
  year = {2003},
  month = apr,
  journal = {Proceedings of the Royal Society of London B: Biological Sciences},
  volume = {270},
  number = {1517},
  pages = {819--826},
  issn = {0962-8452, 1471-2954},
  doi = {10.1098/rspb.2002.2290},
  abstract = {Kin\textendash recognition systems have been hypothesized to exist in humans, and adaptively to regulate altruism and incest avoidance among close genetic kin. This latter function allows the architecture of the kin recognition system to be mapped by quantitatively matching individual variation in opposition to incest to individual variation in developmental parameters, such as family structure and co\textendash residence patterns. Methodological difficulties that appear when subjects are asked to disclose incestuous inclinations can be circumvented by measuring their opposition to incest in third parties, i.e. morality. This method allows a direct test of Westermarck's original hypothesis that childhood co\textendash residence with an opposite\textendash sex individual predicts the strength of moral sentiments regarding third\textendash party sibling incest. Results support Westermarck's hypothesis and the model of kin recognition that it implies. Co\textendash residence duration objectively predicts genetic relatedness, making it a reliable cue to kinship. Co\textendash residence duration predicts the strength of opposition to incest, even after controlling for relatedness and even when co\textendash residing individuals are genetically unrelated. This undercuts kin\textendash recognition models requiring matching to self (through, for example, major histocompatibility complex or phenotypic markers). Subjects' beliefs about relatedness had no effect after controlling for co\textendash residence, indicating that systems regulating kin\textendash relevant behaviours are non\textendash conscious, and calibrated by co\textendash residence, not belief.},
  language = {en},
  pmid = {12737660},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/DQAIFDH6/Lieberman et al_2003_Does morality have a biological basis.pdf;/media/cillian/intel250G/Home/Google/Zotero/storage/7XNTXDCB/819.html}
