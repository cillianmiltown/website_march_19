  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/NUGADZVG/Rosch et al_1976_Basic objects in natural categories.pdf;/media/cillian/intel250G/Home/Google/Zotero/storage/GWSVV6ER/001002857690013X.html}
}

