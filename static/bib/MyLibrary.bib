  author = {Chapman, Hanah A. and Anderson, Adam K.},
  file = {/home/cillian/Google/Zotero/storage/2WPHDIUG/Chapman_Anderson_2014_Trait physical disgust is related to moral judgments outside of the purity.pdf;/home/cillian/Google/Zotero/storage/C8QGTD2Z/Chapman_Anderson_2014_Trait physical disgust is related to moral judgments outside of the purity.pdf},
