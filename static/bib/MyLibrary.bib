  keywords = {Norms,Uncertainty,Fairness,Values,Groups,Justice,Mortality,Worldview defense},
  author = {van den Bos, Kees and Poortvliet, P. Marijn and Maas, Marjolein and Miedema, Joost and van den Ham, Ernst-Jan},
