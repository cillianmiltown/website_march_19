@article{earnshaw_covid19_2020,
  title = {{{COVID}}-19 Conspiracy Beliefs, Health Behaviors, and Policy Support},
  author = {Earnshaw, Valerie A and Eaton, Lisa A and Kalichman, Seth C and Brousseau, Natalie M and Hill, E Carly and Fox, Annie B},
  year = {2020},
