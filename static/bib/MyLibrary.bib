  urldate = {2014-03-04},
  date = {2013-03},
  pages = {180-193},
  keywords = {Between Groups Design,between-group variability,choice behavior,DECISION making,exploration–exploitation,Exploratory Behavior,frequency of losses,Gambling,Group Differences,Iowa Gambling Task,Psychological Assessment,within-group variability},
  author = {Steingroever, Helen and Wetzels, Ruud and Horstmann, Annette and Neumann, Jane and Wagenmakers, Eric-Jan}
}

@article{prinz_putting_2006,
  title = {Putting the {{Brakes}} on {{Enactive Perception}}.},
  volume = {12},
  issn = {1039-723X},
  url = {jesse@subcortex.com},
  abstract = {Comments on the book Action in Perception (see record 2005-01877-000), as summarized by the author (see record 2006-05804-001). Alva Noë's Action in Perception offers a provocative and vigorous defense of the thesis that vision is enactive: visual experience depends on dispositional motor responses. On this view, vision and action are inextricably bound. In this review, I argue against enactive perception. I raise objections to seven lines of evidence that appear in Noë's book, and I indicate some reasons for thinking that vision can operate independently of motor responses. I conclude that the relationship between vision and action is causal, not constitutive. I then address three other contentious hypotheses in the book. Noë argues that visual states are not pictorial; he argues that all perception is conceptual; and he argues that the external world makes a constitutive contribution to experience. I am unpersuaded by these arguments, and I offer reasons to resist Noë's conclusions. (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
