  shorttitle = {A Hot/Cool-System Analysis of Delay of Gratification},
  author = {Metcalfe, Janet and Mischel, Walter},
  year = {1999},
  journal = {Psychological Review},
  volume = {106},
  number = {1},
  pages = {3--19},
  issn = {1939-1471(Electronic);0033-295X(Print)},
  doi = {10.1037/0033-295X.106.1.3},
  abstract = {A 2-system framework is proposed for understanding the processes that enable\textemdash and undermine\textemdash self-control or "willpower" as exemplified in the delay of gratification paradigm. A cool, cognitive "know" system and a hot, emotional "go" system are postulated. The cool system is cognitive, emotionally neutral, contemplative, flexible, integrated, coherent, spatiotemporal, slow, episodic, and strategic. It is the seat of self-regulation and self-control. The hot system is the basis of emotionality, fears as well as passions\textemdash impulsive and reflexive\textemdash initially controlled by innate releasing stimuli (and, thus, literally under "stimulus control"); it is fundamental for emotional (classical) conditioning and undermines efforts at self-control. The balance between the hot and cool systems is determined by stress, developmental level, and the individual's self-regulatory dynamics. The interactions between these systems allow explanation of findings on willpower from 3 decades of research.},
  copyright = {(c) 2016 APA, all rights reserved},
  keywords = {*Cognition,*Delay of Gratification,*Emotions,*Self-Control,*Systems Theory,Cognitive Processes,Emotionality (Personality)}
}

