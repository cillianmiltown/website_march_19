  keywords = {Intuition,Moral foundations theory,Moral judgment,Vignettes,Violations},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/9PF5REFJ/Clifford et al. - 2015 - Moral foundations vignettes a standardized stimul.pdf;/media/cillian/intel250G/Home/Google/Zotero/storage/XSMRPAHY/Clifford et al. - 2015 - Moral foundations vignettes a standardized stimul.pdf}
