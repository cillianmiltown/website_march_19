@phdthesis{lartigue_perceptions_2001,
  title = {Perceptions of Sexual Harassment:  {{The}} Influences of Physical Attractiveness of the Victim and Comparative Status},
  shorttitle = {Perceptions of Sexual Harassment},
  author = {Lartigue, Eva Kathryn Carter},
  year = {2001},
  month = aug,
  abstract = {The present study was an investigation of the effect of specific personal and situational factors on the perception of sexual harassment. Undergraduate psychology students watched a videotaped interaction of a male and a female in a work setting. The male made a comment to the female regarding her dress. Following the viewing of the videotape, the participants completed a paper and pencil questionnaire designed to assess their perceptions of the incident. The questionnaire also contained several instruments that measure aspects of sexism, tolerance for sexual harassment, attitudes toward women, and empathy. The physical attractiveness of the female, the relative status of the male and female, and the gender of the participant/viewer served as the independent variables. The dependent variables included the participants' ratings of whether the incident was sexual harassment, how serious the incident was, the probable emotions of the female after the interaction, and the actions the female should take after the interaction with the male. In addition, participants' scores on the following scales were also used as dependent variables: Locus of Control, the Sexual Harassment Attitude Scale (a measure of tolerance for sexual harassment), the Attitudes toward Women Scale, the Ambivalent Sexism Inventory, and the Modern Sexism Scale. As hypothesized, the attractiveness of the female did influence participants' perceptions: an attractive female was more likely to be perceived as a victim of sexual harassment than was an unattractive female. The relative power or status of the male and female in the videotape did not prove to be a significant influence. The sex of the participant was shown to be a significant influence: female participants were more likely than male participants to perceive the incident as sexual harassment. Two of the measures utilized in the study proved to be correlated with the participants' perceptions of sexual harassment, and thus may be good predictive tools for future research in the area. The two instruments were the Attitudes toward Women Scale, and the Ambivalent Sexism Inventory-Hostile Sexism Subscale. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  school = {ProQuest Information \& Learning},
  keywords = {comparative status,emotional responses,Emotional Responses,gender differences,Human Sex Differences,physical attractiveness,Physical Attractiveness,sexual harassment,Sexual Harassment,Status}
}

@article{laskov_how_2020,
