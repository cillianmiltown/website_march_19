  author = {Abbott, K. and Ellis, S. J. and Abbott, R.},
  file = {/home/cillian/Google/Zotero/storage/SBVE95KR/Abbott et al. - 2016 - 'We've got a lack of family values' an examinatio.pdf;/home/cillian/Google/Zotero/storage/JKT8UWF3/494.html}
