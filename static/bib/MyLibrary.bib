@article{barsalou_situated_2003,
  title = {Situated Simulation in the Human Conceptual System},
  author = {Barsalou, Lawrence W.},
  year = {2003},
  journal = {Language and Cognitive Processes},
  volume = {18},
  number = {5-6},
  pages = {513--562},
  issn = {0169-0965},
  doi = {10.1080/01690960344000026},
  abstract = {Four theories of the human conceptual system\textemdash semantic memory, exemplar models, feed-forward connectionist nets, and situated simulation theory\textemdash are characterised and contrasted on five dimensions: (1) architecture (modular vs. non-modular), (2) representation (amodal vs. modal), (3) abstraction (decontextualised vs. situated), (4) stability (stable vs. dynamical), and (5) organisation (taxonomic vs. action\textendash environment interface). Empirical evidence is then reviewed for the situated simulation theory, and the following conclusions are reached. Because the conceptual system shares mechanisms with perception and action, it is non-modular. As a result, conceptual representations are multi-modal simulations distributed across modality-specific systems. A given simulation for a concept is situated, preparing an agent for situated action with a particular instance, in a particular setting. Because a concept delivers diverse simulations that prepare agents for action in many different situations, it is dynamical. Because the conceptual system's primary purpose is to support situated action, it becomes organised around the action\textendash environment interface.},
  keywords = {Read,Simulation,situated cognition},
  annotation = {00523},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/CE2DRDU3/Barsalou_2003_Situated simulation in the human conceptual system.pdf;/media/cillian/intel250G/Home/Google/Zotero/storage/U424AHHJ/BarsalouSituatedSimulation2003.pdf;/media/cillian/intel250G/Home/Google/Zotero/storage/RXNGZM59/01690960344000026.html}
