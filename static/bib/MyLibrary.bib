  shorttitle = {Is {{Empathic Accuracy Enough}} to {{Facilitate Responsive Behavior}} in {{Dyadic Interaction}}?},
  author = {Winczewski, Lauren A. and Bowen, Jeffrey D. and Collins, Nancy L.},
  year = {2016},
  month = mar,
  journal = {Psychological Science},
  volume = {27},
  number = {3},
  pages = {394--404},
  publisher = {{SAGE Publications Inc}},
  issn = {0956-7976},
  doi = {10.1177/0956797615624491},
  abstract = {Growing evidence suggests that interpersonal responsiveness\textemdash feeling understood, validated, and cared for by other people\textemdash plays a key role in shaping the quality of one's social interactions and relationships. But what enables people to be interpersonally responsive to others? In the current study, we argued that responsiveness requires not only accurate understanding but also compassionate motivation. Specifically, we hypothesized that understanding another person's thoughts and feelings (empathic accuracy) would foster responsive behavior only when paired with benevolent motivation (empathic concern). To test this idea, we asked couples (N = 91) to discuss a personal or relationship stressor; we then assessed empathic accuracy, empathic concern, and responsive behavior. As predicted, when listeners' empathic concern was high, empathic accuracy facilitated responsiveness; but when empathic concern was low, empathic accuracy was unhelpful (and possibly harmful) for responsiveness. These findings provide the first evidence that cognitive and affective forms of empathy work together to facilitate responsive behavior.},
  language = {en},
  keywords = {empathic accuracy,empathic concern,empathy,open data,open materials,responsiveness,understanding},
  file = {/media/cillian/intel250G/Home/Google/Zotero/storage/6WF7RCKV/Winczewski et al_2016_Is Empathic Accuracy Enough to Facilitate Responsive Behavior in Dyadic.pdf}
}

