@article{hyde_measure_2003,
  title = {A Measure of Quality of Life in Early Old Age: {{The}} Theory, Development and Properties of a Needs Satisfaction Model ({{CASP}}-19)},
  volume = {7},
  issn = {1360-7863},
  url = {https://doi.org/10.1080/1360786031000101157},
  doi = {10.1080/1360786031000101157},
  shorttitle = {A Measure of Quality of Life in Early Old Age},
  abstract = {Quality of life is the subject of much research. However it lacks an agreed theoretical basis. In studies with older populations (ill) health has been used as a proxy measure for quality of life (QoL). We have developed a needs satisfaction measure of QoL in early old age. Our measure has four ontologically grounded domains: control, autonomy, pleasure, and self-realization. The measure was piloted with focus groups, a self-completion pilot, and cognitive interview testing. This produced a 22-item scale that was included in a postal questionnaire and sent to 286 people aged 65-75 years. A 92\% response rate was achieved. The scale was reduced to 19 items on the basis of statistical analysis. The domains have Cronbach's alphas between 0.6 and 0.8. Correlations between the four domains range from 0.4 to 0.7. A second order factor analysis revealed a single latent QoL factor. The scores for the 19-item scale are well distributed along the range although they exhibit a slight negative skew. Concurrent validity was assessed using the Life Satisfaction Index–well being. A strong and positive association was found between the two scales (r = 0.6, p = 0.01). The CASP-19 appears to be a useful scale for measuring QoL in older people.},
  number = {3},
  journaltitle = {Aging \& Mental Health},
  urldate = {2019-04-07},
  date = {2003-05-01},
  pages = {186-194},
  author = {Hyde, M. and Wiggins, R. D. and Higgs, P. and Blane, D. B.},
  file = {/home/cillian/Google/Zotero/storage/IJMS5BNJ/Hyde et al_2003_A measure of quality of life in early old age.pdf;/home/cillian/Google/Zotero/storage/WEFVKC74/1360786031000101157.html},
