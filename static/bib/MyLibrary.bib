@incollection{fabri_useful_2016,
  title = {Useful {{Tools}}},
  booktitle = {Measurement and {{Analysis}} in {{Transforming Healthcare Delivery}}: {{Volume}} 1: {{Quantitative Approaches}} in {{Health Systems Engineering}}},
  author = {Fabri, Peter J.},
  editor = {Fabri, Peter J.},
  year = {2016},
  pages = {145--152},
  publisher = {{Springer International Publishing}},
  address = {{Cham}},
  doi = {10.1007/978-3-319-40812-5_11},
  abstract = {This chapter presents several particularly useful tools for the analyst. It is not intended to be comprehensive, as the commonly used tools are readily available in the references and bibliography. Rather, these tools, while uncommonly discussed in the analytics literature, may provide new approaches to some common problems in medical data analysis and interpretation. (1) Outliers are data points that are so ``distant'' from expected as to suggest that they belong elsewhere. Often, this concept is used to eliminate noisy data points. However, it is not uncommon for outliers to actually be more interesting than the core data. Outliers, therefore, need to be considered in both lights: nuisance versus interest. (2) Statistical process control (SPC) is a set of methods developed in the manufacturing industry which can be applied in a number of medical analytical situations to visually and statistically identify when a substantive change has occurred in a ``before/after'' situation. It was originally created to allow individuals with minimal mathematical skills to be able to track a manufacturing process for drift or error. It lends itself well to tracking the occurrence of events over time, such as wound infections and readmissions. (3) Failure mode and effect analysis (FMEA) is a technique developed in industry to identify before a new piece of equipment of process is activated how that entity could fail. The technique uses a brainstorming methodology and a set of process experts to identify the most likely failure modes and the magnitude of the impact they would likely have.},
  isbn = {978-3-319-40812-5},
  language = {en},
  keywords = {Control levels,Delphi process,Failure mode and effects,Nominal group process,Out of control,Outliers,Statistical process control,Western electric rules}
}

