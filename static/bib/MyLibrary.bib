  abstract = {Political misperceptions can distort public debate and undermine people's ability to form meaningful opinions. Why do people often hold these false or unsupported beliefs, and why is it sometimes so difficult to convince them otherwise? We argue that political misperceptions are typically rooted in directionally motivated reasoning, which limits the effectiveness of corrective information about controversial issues and political figures. We discuss factors known to affect the prevalence of directionally motivated reasoning and assess strategies for accurately measuring misperceptions in surveys. Finally, we address the normative implications of misperceptions for democracy and suggest important topics for future research.},
  number = {S1},
  journaltitle = {Political Psychology},
  urldate = {2019-12-04},
  date = {2017},
  pages = {127-150},
  keywords = {motivated reasoning,experiments,corrections,misinformation,misperceptions},
  author = {Flynn, D. J. and Nyhan, Brendan and Reifler, Jason},
  file = {/home/cillian/Google/Zotero/storage/QC7BRMTL/Flynn et al_2017_The Nature and Origins of Misperceptions.pdf;/home/cillian/Google/Zotero/storage/JKG2XBZ4/pops.html}
}

@article{rozenblit_misunderstood_2002,
  langid = {english},
  title = {The Misunderstood Limits of Folk Science: An Illusion of Explanatory Depth},
  volume = {26},
  issn = {1551-6709},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1207/s15516709cog2605_1},
  doi = {10.1207/s15516709cog2605_1},
  shorttitle = {The Misunderstood Limits of Folk Science},
  abstract = {People feel they understand complex phenomena with far greater precision, coherence, and depth than they really do; they are subject to an illusion—an illusion of explanatory depth. The illusion is far stronger for explanatory knowledge than many other kinds of knowledge, such as that for facts, procedures or narratives. The illusion for explanatory knowledge is most robust where the environment supports real-time explanations with visible mechanisms. We demonstrate the illusion of depth with explanatory knowledge in Studies 1–6. Then we show differences in overconfidence about knowledge across different knowledge domains in Studies 7–10. Finally, we explore the mechanisms behind the initial confidence and behind overconfidence in Studies 11 and 12, and discuss the implications of our findings for the roles of intuitive theories in concepts and cognition.},
  number = {5},
  journaltitle = {Cognitive Science},
  urldate = {2019-12-04},
  date = {2002},
  pages = {521-562},
  keywords = {Concepts,Epistemology,Knowledge,Overconfidence,Meta-cognition},
  author = {Rozenblit, Leonid and Keil, Frank},
  file = {/home/cillian/Google/Zotero/storage/V4PF5BVT/Rozenblit_Keil_2002_The misunderstood limits of folk science.pdf;/home/cillian/Google/Zotero/storage/RVMNT6WI/s15516709cog2605_1.html}
}

