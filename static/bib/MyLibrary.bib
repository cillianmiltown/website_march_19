@article{banet-weiser_ruined_2021,
  title = {`{{Ruined}}' Lives: {{Mediated}} White Male Victimhood},
  shorttitle = {`{{Ruined}}' Lives},
  author = {{Banet-Weiser}, Sarah},
  year = {2021},
